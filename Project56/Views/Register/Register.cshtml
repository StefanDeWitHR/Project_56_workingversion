@model Users

<div class="row">
    <div class="col-md-10 ">
        @using (Html.BeginForm("Register", "Register", FormMethod.Post ))
        {
            <legend>Registeren</legend>
                <div class="form-group">
                        <div class="col-sm-10">
                            @Html.TextBox("email") 
                           
                        </div>
                        <div class="col-sm-10">
                            @Html.TextBox("password") 
                           
                        </div>
                        <div class="col-sm-4">
                            @html.TextBox("user_level")
                        </div>
                        <div class="col-sm-2">
                            @html.TextBox("Text")
                        </div>
                        <div class="col-sm-5">
                            @html.TextBox("Text")
                        </div>

                    <div class="col-sm-5">
                        @html.TextBox("Geboorte Datum")
                    </div>

                    <div class="col-sm-4">
                        @html.TextBox("gender")
                    </div>

                <legend>Adresgegevens</legend>

                    <div class="col-sm-10">
                        @html.TextBox("AddressLine1")
                    </div>
                
                    <div class="col-sm-10">
                        @html.TextBox("AddressLine2")
                    </div>

                    <div class="col-sm-4">
                        @html.TextBox("City")
                    </div>
                    <div class="col-sm-4">
                        @html.TextBox("PostalCode")
                    </div>
                </div>


                <input type="submit"/>
                <br /> 
        }
                <button type="submit">Registreren</button>
            </fieldset>

